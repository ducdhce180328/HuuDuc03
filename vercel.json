{
    "version": 2,
    "cleanUrls": true,
    "trailingSlash": false,
    "headers": [
        {
            "source": "/(.*)\\.(png|jpg|jpeg|gif|webp|svg|mp3)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/index_fixed.html"
        }
    ]
}